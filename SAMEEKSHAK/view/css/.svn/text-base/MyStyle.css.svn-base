#header {
	background-image: url(/svarn/view/Images/mactut8.jpg);
	background-repeat: repeat-x;
	width: 1100px;
	height: 60px;
	-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	border-radius: 20px;
	/*border: 1px solid;*/
	margin-top: 1%;
	margin-left: 7%;
}

 #header1 {
	background-position: center;
	background-image: url(/sameekshak/view/images/header_connector.png);
	background-repeat: repeat-x ;
	width: 1100px;
	height: 60px;
	-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	border-radius: 20px;
	border: 0px solid blue;
	margin-top: 1%;
	margin-left: 7%;
}

a {
	color: #046A7A;
}

.sameekname {
	float: left;
	font-size: 35px;
	color: #ffffff;
}

#content {
	background: url("/sameekshak/view/images/blue-background.png") repeat scroll 0 0 #EBF0F6;
	width: 1100px;
	margin-top: 3px;
	moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	border: thin solid blue;
	border-radius: 20px;
	margin-left: 7%;
}

.content_color {
	background: #95d973;
	background-position: top;
	z-index: -10;
	-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	border-radius: 20px;
	border: 15px threeddarkshadow #8fd46c;
}

.content_color1 {
	background: #95d973;
	background-position: top;
	background-image: url(/svarn/view/Images/cd_sdfglass.png);
	z-index: -10;
	-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	border-radius: 20px;
	border: 15px threeddarkshadow #8fd46c;
}
#footer {
	background-image: url(/svarn/view/Images/mactut8.jpg);
	width: 1100px;
	margin-top: 3px;
	margin-left: 7%;
	border: 1px solid blue;
}


