<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<form action="/sameekshak/createStudent" method="post" enctype="multipart/form-data">
	<table align="center">
		<tr>
			<td>Student Id</td>
			<td>:</td>
			<td><input type="text" id="studentId" name="studentId" size="15"
				maxlength="15" class="txtfield" /></td>
		</tr>
		<tr>
			<td>Course Id</td>
			<td>:</td>
			<td><select name="courseId" class="txtfield">
					<option value="" selected="selected">--Select--</option>
					<option value="111">FPGDST</option>
					<option value="112">FPGDIS</option>
			</select></td>
		</tr>
		<tr>
			<td>Student Name</td>
			<td>:</td>
			<td><input type="text" id="studentName" name="studentName"
				size="25" maxlength="100" class="txtfield" /></td>
		</tr>
		<tr>
			<td>Email Id</td>
			<td>:</td>
			<td><input type="text" id="studentEmailId" name="studentEmailId"
				size="25" maxlength="100" class="txtfield" /></td>
		</tr>
		<tr>
			<td>Contact No.</td>
			<td>:</td>
			<td><input type="text" id="studentPhoneNo" name="studentPhoneNo"
				size="10" maxlength="10" class="txtfield" /></td>
		</tr>
		<tr>
			<td>Image</td>
			<td>:</td>
			<td><input type="file" id="image" name="image" size="25"
				class="txtfield" /></td>
		</tr>
		<tr>
			<td colspan="3"><input type="submit" id="submit" name="submit"
				value="Create Admin" /></td>
		</tr>
	</table>
</form>